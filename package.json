{
	"name": "react-native-toast-message",
	"version": "2.0.0",
	"description": "Toast message component for React Native",
	"main": "index.js",
	"scripts": {
		"hooks:install": "node ./node_modules/husky/bin/install",
		"lint": "./node_modules/.bin/eslint . --ext=jsx --ext=js",
		"lint-staged": "./node_modules/.bin/lint-staged",
		"prettier": "./node_modules/.bin/prettier . --write"
	},
	"lint-staged": {
		"**/*.(js|ts|jsx|tsx)": [
			"prettier --write",
			"eslint"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"react-native",
		"toast"
	],
	"author": "StratusCode <contact@stratuscode.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StratusCode/react-native-toast-message.git"
	},
	"peerDependencies": {
		"react": ">=16.13.0",
		"react-native": ">=0.63.0",
		"react-native-svg": "12.1.0"
	},
	"devDependencies": {
		"@react-native-community/eslint-config": "^2.0.0",
		"@types/react": "^16.13.0",
		"@types/react-native": "^0.63.0",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.11.0",
		"eslint-import-resolver-typescript": "^2.3.0",
		"eslint-plugin-import": "^2.20.1",
		"husky": "^3.1.0",
		"lint-staged": "^10.2.2",
		"prettier": "2.1.2",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"react": "^16.14.0",
		"react-native": "^0.63.3",
		"react-native-svg": "^12.1.0"
	}
}
